{
	"name": "my-app",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@apollo/react-hooks": "3.1.3",
		"apollo-boost": "0.4.7",
		"graphql": "14.5.8",
		"node-sass": "4.12.0",
		"react": "16.9.0",
		"react-dom": "16.9.0",
		"react-router-dom": "5.0.1",
		"react-scripts": "3.1.2"
	},
	"devDependencies": {
		"@types/node": "12.7.2",
		"@types/react": "16.9.2",
		"@types/react-dom": "16.8.5",
		"@types/react-router-dom": "4.3.4",
		"identity-obj-proxy": "3.0.0",
		"prettier": "1.19.1",
		"tslint": "5.18.0",
		"tslint-eslint-rules": "5.4.0",
		"tslint-react": "4.0.0",
		"typescript": "3.5.3"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"lint": "tslint --project tsconfig.json './src/**/*[^.d].ts*'",
		"codegen": "apollo codegen:generate --target typescript --excludes=node_modules/* --includes='**/*.tsx' --endpoint='http://localhost:4000' --tagName=gql --outputFlat=src/__generated__"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
